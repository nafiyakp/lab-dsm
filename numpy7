import numpy as np
x=np.array([[1,0],[0,1]])
print("array")
print(x)
print("sum of all elements")
print(np.sum(x))
print("sum of each column")
print(np.sum(x,axis=0))
print("sum of each row")
print(np.sum(x,axis=1))



array
[[1 0]
 [0 1]]
sum of all elements
2
sum of each column
[1 1]
sum of each row
[1 1]
